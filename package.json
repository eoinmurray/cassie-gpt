  "scripts": {
    "dev": "next dev",
    "test": "bun ./src/tests/test.ts",
    "build": "bun ./src/actions/remote-load-env.ts; prisma generate > /dev/null && echo 'prisma generate finished'; next build",
    "deploy": "vercel --prod --logs",
    "start": "next start",
    "pushenv": "npx dotenv-vault push; npx dotenv-vault push production",
    "pullenv": "npx dotenv-vault pull; npx dotenv-vault pull production",
    "wipedb": "npx prisma db push --force-reset",
    "resetdb": "npm run wipedb; bun src/actions/seed.ts",
    "clean": "npm run openaiwipe; run wipedb; bun src/actions/seed.ts ; npm run updateassistants",
    "updateassistants": "bun src/actions/system-update-assistants.ts",
    "openaiwipe": "bun src/actions/openai-wipe.ts",
    "vlogs": "npx vercel logs testbot.e01n.dev -f",
    "vls": "npx vercel list",
    "wranglerdev": "wrangler dev --remote",
    "wranglerdeploy": "wrangler deploy",
    "wranglerlogs": "wrangler tail"
  },
